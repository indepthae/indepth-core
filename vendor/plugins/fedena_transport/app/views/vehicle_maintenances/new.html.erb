<div id="content-header">
    <%= show_header_icon %>
    <h1><%= t('transport_text') %></h1>
    <div class='header-sep'>|</div>
    <div class='sub-header'><%= t('vehicle_maintenance_text') %></div>

</div>
<div id="page-yield">
    <div class="bread_crumb">
        <%= make_breadcrumb %>
        <%= render_breadcrumbs  %>
    </div>
    <% unless flash[:notice].nil? %>
      <p class="flash-msg"> <%= flash[:notice] %> </p>
    <% end %>
    <%= render :partial => 'vehicle_maintenance_form' %>
</div>
