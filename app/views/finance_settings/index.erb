<% if rtl? %>
  <%= stylesheet_link_tag 'rtl/module_dashboard' %>
<% else %>
  <%= stylesheet_link_tag 'module_dashboard' %>
<% end %>

<div id="content-header">
    <%= show_header_icon %>
    <h1><%= t('finance_text') %></h1>
    <div class='header-sep'>|</div>
    <div class='sub-header'><%= t('fee_settings') %></div>
</div>

<div id="page-yield">
    <div class="bread_crumb">
        <%= make_breadcrumb %>
        <%= render_breadcrumbs  %>
    </div>

    <% unless flash[:notice].nil? %>
      <p class="flash-msg">
          <%= flash[:notice] %>
      </p>
    <% end %>
    <div class="box">
        <div class="link-box">
            <div class="link-heading">
                <%= link_to "#{t('fee_settings')}", :action => "fee_settings" %>
            </div>
            <div class="link-descr"><%= t('fee_settings_descr') %></div>
        </div>
    </div>
</div>