<% default_configs ||= nil %>
<% if (template_id.present? and @data[:templates].has_key?(template_id.to_i)) %>

  <%= render :partial => "receipt_templates/receipt_header",     
    :locals => {:header_content => @data[:templates][template_id.to_i].try(:last), :printer => true }%>

<% else %>    

  <%= render :partial => "finance_extensions/receipts/defaults/receipt_header", 
    :locals => {:default_configs => default_configs} %>

<% end %>







