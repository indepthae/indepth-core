<div class="label-field-pair date_field">
    <label for="start_date"><%= t('start_date') %></label>
    <div class="text-input-bg">
        <%= calendar_date_select_tag 'exam_dates[start_date]','',
          :readonly=>true, :popup=>"force", :id => 'start_date', :onchange=>"validateDateRange()" %>
    </div>
</div>
<div class="label-field-pair date_field">
    <label for="end_date"><%= t('end_date') %></label>
    <div class="text-input-bg">
        <%= calendar_date_select_tag 'exam_dates[end_date]','',
          :readonly=>true, :popup=>"force", :id => 'end_date', :onchange=>"validateDateRange()" %>
    </div>
</div>